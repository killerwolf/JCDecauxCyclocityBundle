<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="cyclocity.data" class="JCDodatawrapper\Vls\Wrapper">
            <argument>%killerwolf_jc_decaux_cyclocity.api_key%</argument>
            <call method="setHTTPClient">
                <argument type="service" id="cyclocity.http_client" />
            </call>
        </service>
        <service id="cyclocity.http_client" class="JCDodatawrapper\Vls\HTTPClientCurl">
        </service>
    </services>
</container>
